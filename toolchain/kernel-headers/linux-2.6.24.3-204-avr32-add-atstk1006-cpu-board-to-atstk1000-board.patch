@@ -0,0 +1,72 @@
+/*
+ * ATSTK1006 board-specific NAND flash initialization
+ *
+ * Copyright (C) 2008 Atmel Corporation
+ *
+ * This program is free software; you can redistribute it and/or modify it
+ * under the terms of the GNU General Public License version 2 as published by
+ * the Free Software Foundation.
+ */
+#include <linux/init.h>
+#include <linux/mtd/mtd.h>
+#include <linux/mtd/partitions.h>
+
+#include <asm/arch/smc.h>
+#include <asm/arch/board.h>
+#include <asm/arch/at32ap700x.h>
+
+static struct smc_timing nand_timing __initdata = {
+	.ncs_read_setup		= 0,
+	.nrd_setup		= 0,
+	.ncs_write_setup	= 0,
+	.nwe_setup		= 0,
+
+	.ncs_read_pulse		= 20,
+	.nrd_pulse		= 15,
+	.ncs_write_pulse	= 20,
+	.nwe_pulse		= 15,
+
+	.read_cycle		= 30,
+	.write_cycle		= 30,
+};
+
+static struct smc_config nand_config __initdata = {
+	.bus_width		= 1,
+	.nrd_controlled		= 1,
+	.nwe_controlled		= 1,
+};
+
+static struct mtd_partition nand_parts[] = {
+	{
+		.name           = "nand",
+		.offset         = 0x00000000,
+		.size           = 0x10000000,
+	},
+};
+
+static struct at32_nand_data nand_data = {
+	.enable_pin	= GPIO_PIN_PB(29),
+	.rdy_pin	= GPIO_PIN_PB(30),
+	.ale		= 22,
+	.cle		= 21,
+	.nr_parts	= ARRAY_SIZE(nand_parts),
+	.parts		= nand_parts,
+};
+
+/* This needs to be called after the SMC has been initialized */
+static int __init atstk1006_nand_init(void)
+{
+	int ret;
+
+	smc_set_timing(&nand_config, &nand_timing);
+	ret = smc_set_configuration(3, &nand_config);
+	if (ret < 0) {
+		printk(KERN_ERR "atstk1006: failed to set NAND flash timing\n");
+		return ret;
+	}
+
+	at32_add_device_nand(0, &nand_data);
+
+	return 0;
+}
+device_initcall(atstk1006_nand_init);
