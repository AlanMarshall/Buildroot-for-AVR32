comment "Atmel board support"

choice
	prompt "AVR32 board type"
	default BR2_TARGET_ATMEL_ATSTK1000

config BR2_TARGET_ATMEL_ATSTK1000
	bool "ATSTK1000 development board"
	depends BR2_avr32
	help
	  Support for the ATSTK1000 development board for Atmel AVR32.

config BR2_TARGET_ATMEL_ATNGW100
	bool "ATNGW100 reference board"
	depends BR2_avr32
	help
	  Support for the ATNGW100 reference board for Atmel AVR32.

endchoice
